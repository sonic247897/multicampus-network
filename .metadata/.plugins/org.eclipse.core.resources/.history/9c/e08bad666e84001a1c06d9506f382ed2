package basic;

import java.net.MalformedURLException;
import java.net.URL;

// URL 클래스
//	=> 인터넷에서 확인할 수 있는 자원을 추출
public class URLTest01 {
	public static void main(String[] args) {
		// 네트워크 상에서 컴퓨터 = 노드, 호스트
		try {
			URL url = new URL("https://www.daum.net");
		} catch (MalformedURLException e) {
			e.printStackTrace();
		}
	}
}
